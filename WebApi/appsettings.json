{
  "AppConfiguration": {
    "ApiName": "KRFTemplateApi",
    "TokenIdentifier": "AccessToken",
    "TokenKey": "APICOMMONSIGNEDKEY"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=KRFTemplateApiDB;AttachDbFilename=DBFiles\\KRFTemplateApiDB.mdf;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "UseLocalDB": true,
  "ApiDBFolder": "DBFiles",
  "MigrationAssembly": "KRFTemplateApi.Infrastructure",
  "KestrelConfiguration": {
    "HttpPort": "4747",
    "HttpsPort": "14747"
  },
  "RequestContext": {
    "EnableRead": false,
    "MemBufferOnly": true,
    "BufferSize": "0"
  },
  "LogsOnPrd": false
}
